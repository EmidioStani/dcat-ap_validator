# Rule_ID:56
# @title [Dataset] dct:conformsTo should be a dct:Standard
# @tag dataset
# @endpoint http://localhost:3030/dcat-ap_validator/query

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT ?Rule_Origin ?Rule_ID ?Rule_Severity ?Rule_Description ?Message ?s (dct:publisher as ?p) ?o 
WHERE {
  ?s a dcat:Dataset.
  ?s dct:publisher ?o.
  ?o ?pred ?val.
  FILTER(!EXISTS {?o a dct:Standard}).
  BIND ("DCAT-AP" AS ?Rule_Origin).
  BIND (56 AS ?Rule_ID).
  BIND ("error" AS ?Rule_Severity).
  BIND ("dct:conformsTo should be a dct:Standard." AS ?Rule_Description).
  BIND (concat("The dcat:conformsTo ",str(?o)," is not a dct:Standard.") AS ?Message).
}
