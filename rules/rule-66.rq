# Rule_ID:66
# @title [Dataset] adms:identifier should be a literal
# @tag dataset
# @endpoint http://localhost:3030/dcat-ap_validator/query

PREFIX adms: <http://www.w3.org/ns/adms#>
PREFIX dcat: <http://www.w3.org/ns/dcat#>

SELECT ?Rule_Origin ?Rule_ID ?Rule_Severity ?Rule_Description ?Message ?s ?p ?o
WHERE {
  ?s a dcat:Dataset.
  ?s adms:identifier ?o.
  FILTER(!isLiteral(?o)).
  BIND (adms:identifier AS ?p).
  BIND ("DCAT-AP" AS ?Rule_Origin).
  BIND (66 AS ?Rule_ID).
  BIND ("error" AS ?Rule_Severity).
  BIND ("adms:identifier should be a literal." AS ?Rule_Description).
  BIND (concat("The adms:identifier ",str(?p)," is not a literal.") AS ?Message).
}