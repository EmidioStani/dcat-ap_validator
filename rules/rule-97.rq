# Rule_ID:97
# @title [Dataset] dct:temporal should be resource
# @tag dataset
# @endpoint http://localhost:3030/dcat-ap_validator/query

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT ?Rule_Origin ?Rule_ID ?Rule_Severity ?Rule_Description ?Message ?s ?p ?o 
WHERE {
  ?s a dcat:Dataset.
  ?s dct:temporal ?o.
  FILTER(!isURI(?o)).
  BIND (dct:temporal AS ?p).
  BIND ("DCAT-AP" AS ?Rule_Origin).
  BIND (97 AS ?Rule_ID).
  BIND ("error" AS ?Rule_Severity).
  BIND ("dct:temporal should be resource." AS ?Rule_Description).
  BIND (concat("The dct:temporal ",str(?s)," is not a resource.") AS ?Message).
}

