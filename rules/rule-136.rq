# Rule_ID:136
# @title [Checksum] schema:startDate has a maximum cardinality of 1 for dct:PeriodOfTime.
# @tag Checksum
# @endpoint http://localhost:3030/dcat-ap_validator/query

PREFIX dct: <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>

SELECT ("DCAT-AP" AS ?Rule_Origin) (136 AS ?Rule_ID) ("error" AS ?Rule_Severity) ("schema:startDate has a maximum cardinality of 1 for dct:PeriodOfTime." AS ?Rule_Description) (concat("The dct:PeriodOfTime ", str(?s)," has more than 1 schema:startDate.") AS ?Message) ?s
WHERE {
  ?s a dct:PeriodOfTime.
  ?s schema:startDate ?mod.
} GROUP BY ?s 
  HAVING (COUNT( ?s) > 1)