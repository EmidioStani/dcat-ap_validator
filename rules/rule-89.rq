# Rule_ID:89
# @title [Distribution] dct:byteSize should be a literal
# @tag distribution
# @endpoint http://localhost:3030/dcat-ap_validator/query

PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT ?Rule_Origin ?Rule_ID ?Rule_Severity ?Rule_Description ?Message ?s ?p ?o 
WHERE {
  ?s a dcat:Distribution.
  ?s dct:byteSize ?o.
  FILTER(!isLiteral(?o)).
  BIND (dct:byteSize AS ?p).
  BIND ("DCAT-AP" AS ?Rule_Origin).
  BIND (89 AS ?Rule_ID).
  BIND ("error" AS ?Rule_Severity).
  BIND ("dct:license should be a resource." AS ?Rule_Description).
  BIND (concat("The dct:license ",str(?s)," is not a resource.") AS ?Message).
}

